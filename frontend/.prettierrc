{
	"bracketSpacing": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"objectWrap": "preserve",
	"bracketSameLine": true,
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"singleAttributePerLine": true,
	"printWidth": 80
}
